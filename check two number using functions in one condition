#include<stdio.h>
void get(int,int);
int main(){
    int a,b;
    printf("Enter the first value (a): ");
    scanf("%d",&a);
    printf("Enter the second value (b): ");
    scanf("%d",&b);
   {
       (a>b)?printf("a is Greater i.e. %d",a):printf("b is Greater i.e. %d",b);
   }
return 0;
  }
  
  
  
  
  Output:
  
Enter the first value (a): 3
Enter the second value (b): 5
b is Greater i.e. 5
